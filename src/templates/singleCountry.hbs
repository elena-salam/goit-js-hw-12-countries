{{#each this}}
<h2>{{name}}</h2>
<div class="container">
    <div class="main-info">
        <h4 class="capital">Capital: <span>{{capital}}</span></h4>
        <h4 class="population">Population: <span>{{population}}</span></h4>
        <h4 class="languages">Languages: </h4>
        <ul class="lang-list">
            {{#each languages}}
            <li class="lang-item">{{name}}</li>
            {{/each}}
        </ul>
    </div>
    <img src="{{flag}}" width="300px" height ="200px" alt=''/>
</div>
{{/each}}
